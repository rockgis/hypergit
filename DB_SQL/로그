CREATE TABLE GITTE0001
(
    ID             INT NOT NULL AUTO_INCREMENT PRIMARY KEY  COMMENT '등록순번',
    DCD             VARCHAR(30) COMMENT '부서ID',
    USR_EN          VARCHAR(30) COMMENT '사번',
    USR_NM          VARCHAR(30) COMMENT '성명',
    GIN_SSS_ID      VARCHAR(50) COMMENT '로그인세션 ID',
    MVE_PGE_URL_AR  VARCHAR(200) COMMENT '이전페이지URL',
    GIN_DT          TIMESTAMP(6) COMMENT '로그인일시',
    GUT_DT          TIMESTAMP(6) COMMENT '로그아웃일시'
);

ALTER TABLE GITTE0001 COMMENT='접속로그';


CREATE TABLE GITTE0002
(
    ID         INT NOT NULL AUTO_INCREMENT PRIMARY KEY COMMENT '등록순번',
    DCD         VARCHAR(30) COMMENT '부서코드',
    USR_EN      VARCHAR(30) COMMENT '사번',
    USR_NM      VARCHAR(30) COMMENT '성명',
    REQ_NM      VARCHAR(200) COMMENT '연동 정보',
    MNU_ID      CHAR(1) COMMENT '결과',
    RG_DT       TIMESTAMP(6) COMMENT '등록일시'
);

ALTER TABLE GITTE0002 COMMENT='연동로그';